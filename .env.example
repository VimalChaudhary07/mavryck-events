# Supabase Configuration
VITE_SUPABASE_URL=https://your-project-id.supabase.co
VITE_SUPABASE_ANON_KEY=your-anon-key

# Production Environment Variables
NODE_ENV=production

# Security Configuration (Optional - defaults are provided)
VITE_ADMIN_EMAIL=admin@mavryckevents.com
VITE_SESSION_TIMEOUT=1800000
VITE_MAX_LOGIN_ATTEMPTS=5
VITE_LOCKOUT_DURATION=900000

# Analytics and Monitoring (Optional)
VITE_ANALYTICS_ID=your-analytics-id
VITE_SENTRY_DSN=your-sentry-dsn

# Email Configuration (Optional - for future email features)
VITE_EMAIL_SERVICE_URL=your-email-service-url
VITE_EMAIL_API_KEY=your-email-api-key

# File Upload Configuration (Optional)
VITE_MAX_FILE_SIZE=5242880
VITE_ALLOWED_FILE_TYPES=image/jpeg,image/png,image/gif,image/webp